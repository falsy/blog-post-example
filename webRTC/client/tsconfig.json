{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "outDir": "./dist/",
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./"
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
